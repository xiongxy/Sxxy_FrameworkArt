@model Sxxy_FrameworkArt.Web.ViewModels.System.SystemRoleViewModels.SystemRoleListViewModel
@using (Bootstrap.PageLayout_BeginRow())
{
    using (Bootstrap.PageLayout_BeginColumn())
    {
        using (Bootstrap.SearcherPanel(x => x, Model.ViewModelId))
        {
            @Bootstrap.TextField(x => x.Searcher.RoleCode)
            @Bootstrap.TextField(x => x.Searcher.RoleName)
        }
    }
    using (Bootstrap.PageLayout_BeginColumn())
    {
        @Bootstrap.TableFor(x => x)
    }
}
